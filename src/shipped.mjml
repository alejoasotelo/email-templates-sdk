    <mj-section mj-class="section">
      <mj-column mj-class="background-hero">
        <mj-text mj-class="title-hero">${{ lang.order_order_name }}$ - ${{ lang.shipped }}$</mj-text>
        <mj-divider mj-class="title-divider-hero"></mj-divider>
        <mj-text>${{ lang.hi_firstname_lastname }}$</mj-text>
        <mj-spacer height="25px"></mj-spacer>
        <mj-text>${{ lang.your_order_with_the_reference_order_name_has_been_shipped }}$</mj-text>
        <mj-text>${{ lang.explicacion_proceso_de_compra }}$</mj-text>
        <mj-text padding-bottom="15px">${{ lang.mensaje_covid }}$</mj-text>
        <mj-spacer height="20px"></mj-spacer>
        <mj-text padding-bottom="15px">${{ lang.thank_you_for_shopping_with_shop_name }}$</mj-text>
      </mj-column>
    </mj-section>

    <mj-include path="./partials/account.mjml" />


    <mj-include path="./partials/guest_to_customer.mjml" />


    <mj-include path="./partials/footer.mjml" />

